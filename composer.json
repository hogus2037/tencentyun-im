{
    "name": "hogus/tencentyun-tim",
    "description": "腾讯云 即时通信IM",
    "keywords": [
        "tencentyun",
        "tim",
        "sdk",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "hogus",
            "email": "hogus2037@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-curl": "*",
        "tencent/tls-sig-api-v2": "^1.1",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/support": "^7.0|^8.0",
        "pimple/pimple": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Hogus\\Tencent\\Tim\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hogus\\Tencent\\Tim\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hogus\\Tencent\\Tim\\TimServiceProvider"
            ]
        }
    }
